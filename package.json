{
  "name": "kinesis-wallet",
  "productName": "Kinesis Wallet",
  "repository": "https://github.com/KinesisNetwork/wallet-desktop",
  "version": "0.3.0",
  "description": "Kinesis compliant hard wallet",
  "main": "src/index.ts",
  "scripts": {
    "pretest": "npm run sass-compile",
    "test": "npm run lint && npm run compile:tsc",
    "predeploy": "npm run sass-compile && npm version $GIT_TAG --no-git-tag-version",
    "deploy:electron": "electron-forge publish",
    "sass-compile": "node-sass src/style.scss src/style.css",
    "build:web": "IS_WEB=true parcel build src/index-web.html",
    "prestart": "rm -rf .cache && npm run sass-compile",
    "start": "electron-forge start",
    "web": "rm -rf .cache && IS_WEB=true parcel src/index-web.html",
    "compile:tsc": "tsc",
    "watch:tsc": "npm run compile:tsc -- -w",
    "lint": "tslint --project tsconfig.json",
    "lint-fix": "tslint --project tsconfig.json --fix"
  },
  "keywords": [],
  "author": "Kinesis",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "publish_targets": {
        "win32": [
          "s3"
        ],
        "darwin": [
          "s3"
        ],
        "linux": [
          "s3"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "ignore": "src\\style\\.scss"
      },
      "electronWinstallerConfig": {
        "name": "KinesisWallet",
        "setupExe": "kinesis-wallet-setup.exe"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "kinesisnetwork",
        "name": "wallet-desktop"
      },
      "s3": {
        "bucket": "kinesis-desktop-wallet",
        "public": true
      }
    }
  },
  "dependencies": {
    "bulma": "0.7.1",
    "bulma-divider": "1.0.4",
    "clipboard": "2.0.1",
    "crypto-js": "3.1.9-1",
    "electron-compile": "6.4.2",
    "electron-devtools-installer": "2.1.0",
    "electron-json-storage": "4.0.2",
    "electron-squirrel-startup": "1.0.0",
    "file-saver": "1.3.8",
    "js-kinesis-sdk": "0.9.7",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-observable": "1.0.0-alpha.2",
    "redux-persist": "5.10.0",
    "redux-persist-electron-storage": "1.1.2",
    "rxjs": "6.1.0",
    "rxjs-compat": "6.1.0",
    "sweetalert": "2.1.0",
    "tsconfig-paths": "3.3.1",
    "tslib": "1.9.0",
    "typesafe-actions": "2.0.0-1"
  },
  "devDependencies": {
    "@types/crypto-js": "3.1.39",
    "@types/electron": "1.6.10",
    "@types/electron-devtools-installer": "2.0.3",
    "@types/electron-json-storage": "4.0.0",
    "@types/file-saver": "1.3.0",
    "@types/lodash": "4.14.108",
    "@types/react": "16.3.13",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "6.0.0",
    "@types/redux-persist": "4.3.1",
    "electron": "1.8.4",
    "electron-forge": "5.2.2",
    "electron-prebuilt-compile": "2.0.2",
    "node-sass": "4.9.0",
    "parcel-bundler": "1.8.1",
    "parcel-plugin-typescript": "0.7.1",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.3"
  }
}
