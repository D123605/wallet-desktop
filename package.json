{
  "name": "kinesis-wallet",
  "productName": "Kinesis Wallet",
  "version": "0.1.0",
  "description": "Stellar compliant hard wallet",
  "main": "src/index.ts",
  "scripts": {
    "sass-compile": "node sass-compile.js",
    "prestart": "npm run sass-compile",
    "prepublish": "npm run sass-compile",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "tslint --project tsconfig.json --force"
  },
  "keywords": [],
  "author": "ABX",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "publish_targets": {
        "win32": [
          "github",
          "s3"
        ],
        "darwin": [
          "github"
        ],
        "linux": [
          "github"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "KinesisWallet"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "bullioncapital",
        "name": "kinesis-wallet"
      },
      "s3": {
        "bucket": "kinesis-wallet",
        "public": false
      }
    }
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "bulma": "^0.6.2",
    "crypto-js": "^3.1.9-1",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.1.0",
    "electron-json-storage": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "js-xdr": "^1.0.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "stellar-sdk": "^0.8.0",
    "sweetalert": "^2.1.0",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/react": "^15.6.14",
    "@types/react-dom": "^15.5.7",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron": "^1.8.2",
    "electron-forge": "^5.0.0",
    "electron-prebuilt-compile": "1.7.11",
    "node-sass": "^4.7.2",
    "tslint": "^5.1.0",
    "typescript": "^2.7.2"
  }
}
